{
  "name": "lippy",
  "version": "0.0.1",
  "description": "A minimal filler text menubar app.",
  "homepage": "https://github.com/kjbrum/lippy#readme",
  "author": "Kyle Brumm <kjbrumm@gmail.com> (http://kylebrumm.com)",
  "main": "menu.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --out dist --arch x64 --platform mas --asar --overwrite",
    "build:all": "electron-packager . --out dist --asar --overwrite --all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjbrum/lippy.git"
  },
  "keywords": [
    "filler",
    "text",
    "placeholder",
    "lorem",
    "ipsum",
    "hipsum",
    "bacon ipsum",
    "loripsum"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kjbrum/lippy/issues"
  },
  "devDependencies": {
    "electron": "^1.3.4",
    "electron-packager": "^8.0.0"
  },
  "dependencies": {
    "jquery": "^3.1.0",
    "menubar": "^5.1.0"
  }
}
